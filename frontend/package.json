{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "NODE_ENV=development nuxt dev",
    "dev:local": "NODE_ENV=development nuxt dev",
    "dev:test": "NODE_ENV=development nuxt dev",
    "dev:win": "set NODE_ENV=development && nuxt dev",
    "dev:local:win": "set NODE_ENV=development && nuxt dev",
    "dev:test:win": "set NODE_ENV=development && nuxt dev",
    "build:prod": "NODE_ENV=production nuxt build",
    "build:prod:win": "set NODE_ENV=production && nuxt build",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "fix-oxc": "npm rebuild oxc-parser || npm uninstall oxc-parser && npm install oxc-parser@latest"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@nuxt/eslint": "^1.7.1",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "^0.11.10",
    "@pinia/nuxt": "^0.11.2",
    "@supabase/supabase-js": "^2.52.1",
    "@unhead/vue": "^2.0.12",
    "@vueuse/nuxt": "^13.5.0",
    "@zxing/library": "^0.21.3",
    "eslint": "^9.31.0",
    "nuxt": "^4.0.1",
    "pinia": "^3.0.3",
    "typescript": "^5.8.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^24.1.0",
    "autoprefixer": "^10.4.21",
    "tailwindcss": "^3.4.17",
    "vue-tsc": "^2.2.12"
  },
  "overrides": {
    "oxc-parser": {
      "optionalDependencies": {
        "@oxc-parser/binding-linux-x64-gnu": "*"
